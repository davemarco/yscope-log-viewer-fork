{
  "problemMatcher": [
    {
      "owner": "stylelint",
      "pattern": [
        {
          "regexp": "^([^\\s].*)$",
          "file": 1
        },
        {
          "loop": true,
          "regexp": "^\\s+((\\d+):(\\d+))?\\s+(✖|⚠)\\s+(.*)\\s(.*)$",
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5,
          "code": 6
        }
      ]
    }
  ]
}