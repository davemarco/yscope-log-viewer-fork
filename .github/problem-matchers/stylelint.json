{
  "problemMatcher": [
    {
      "owner": "stylelint",
      "pattern": [
        {
          "regexp": "^([^\\s].*)$",
          "file": 1
        },
        {
          "loop": true,
          "regexp": "^\\s+(\\d+):(\\d+)\\s+(✖|⚠)\\s+(.*\\S)\\s+(.*)$",
          "line": 1,
          "column": 2,
          "severity": 3,
          "message": 4,
          "code": 5
        }
      ]
    }
  ]
}