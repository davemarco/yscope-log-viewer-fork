{
  "problemMatcher": [
    {
      "owner": "stylelint",
      "pattern": [
        {
          "regexp": "^([^\\s].*)$",
          "file": 1
        },
        {
          "loop": true,
          "regexp": "^\\s*(\\d+):(\\d+)\\s+(.*\\S)\\s+(.*)$",
          "line": 1,
          "column": 2,
          "message": 3,
          "code": 4
        }
      ]
    }
  ]
}